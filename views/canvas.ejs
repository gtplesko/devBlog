
<html>
  <head>
    <%include block/Header.ejs %>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.8.0/p5.min.js"></script>
    <script src="https://cdnjs.cloudflare.com/ajax/libs/p5.js/0.8.0/addons/p5.dom.min.js"></script>
    <script src="https://unpkg.com/ccapture.js@1.1.0/build/CCapture.all.min.js"></script>
    <script src="<%-driver%>"></script>
    <script src="<%-utility%>"></script>
    <style>
      html,
      body {
        margin: 0;
        padding: 0;
      }
      canvas {
        display: block;
      }
    </style>
  </head>
  <body>
    <div class="jumbotron hero-image">
      <div class="container" id="header-bar">
        <h1>Object Disoriented Design</h1>
        <hr/>
      </div>
    </div>
  <!-- <div class="container search-bar fixed-top sticky-top">
    <%include block/Search.ejs%>
  </div> -->



  <div class="container">
    <div class="well">
        <div class="form-group row">
          <div class="col-xs-2">
            <input type="range" min="0" max="65000" value="5000" class="slider input-group" id="seed" link-to="textSeed">
            <input type="number" id="textSeed" class="input-group">
          </div>
          <div class="col-xs-2">
            <input type="range" min="0" max="2000" value="1000" class="slider input-group" id="zoom" link-to="textZoom">
            <input type="number" id="textZoom" class="input-group">
          </div>
          <div class="col-xs-2">
            <input type="range" min="0" max="10000" value="5000" class="slider input-group" id="xControl" link-to="textxControl">
            <input type="number" id="textxControl" class="input-group">
          </div>
          <div class="col-xs-2">
            <input type="range" min="0" max="10000" value="5000" class="slider input-group" id="yControl" link-to="textyControl">
            <input type="number" id="textyControl" class="input-group">
          </div>
          <div class="col-xs-2">
            <input type="range" min="0" max="10000" value="5000" class="slider input-group" id="zControl" link-to="textzControl">
            <input type="number" id="textzControl" class="input-group">
          </div>
        </div>
      </div>
    </div>
  </body>
</html>
